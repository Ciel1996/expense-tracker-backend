{
  "name": "expense-tracker-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/expense-tracker-frontend",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{workspaceRoot}/dist/apps/expense-tracker-frontend"
      ],
      "options": {
        "root": "apps/expense-tracker-frontend",
        "outputPath": "dist/apps/expense-tracker-frontend",
        "turbo": false
      },
      "configurations": {
        "development": {},
        "production": {
          "optimization": true,
          "generatePackageJson": true
        }
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "root": "apps/expense-tracker-frontend",
        "port": 3000,
        "buildTarget": "expense-tracker-frontend:build"
      },
      "configurations": {
        "development": {
          "dev": true
        }
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/expense-tracker-frontend",
        "port": 3000,
        "buildTarget": "expense-tracker-frontend:build:production"
      },
      "configurations": {
        "production": {}
      }
    },
    "lint": {
      "executor": "@nx/eslint:linter",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/expense-tracker-frontend"],
      "options": {
        "jestConfig": "apps/expense-tracker-frontend/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}

{
  "name": "expense-tracker-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/expense-tracker-client/src",
  "projectType": "library",
  "tags": [],
  "implicitDependencies": ["expense_tracker_api"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/expense-tracker-client",
        "main": "libs/expense-tracker-client/src/index.ts",
        "tsConfig": "libs/expense-tracker-client/tsconfig.lib.json",
        "assets": ["libs/expense-tracker-client/*.md"]
      }
    },
    "orval": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "orval --config libs/expense-tracker-client/orval.config.js"
          }
        ]
      }
    }
  }
}
